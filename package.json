    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react-app": "3.1.2",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "compression": "1.7.3",
    "cors": "2.8.4",
    "coveralls": "3.0.1",
    "css-loader": "0.28.7",
    "dotenv": "^5.0.1",
    "dotenv-expand": "4.2.0",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "4.10.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "express": "4.16.3",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "fs-extra": "3.0.1",
    "heroku-ssl-redirect": "0.0.4",
    "html-webpack-plugin": "2.29.0",
    "ignore-styles": "5.0.1",
    "jest": "20.0.4",
    "js-cookie": "^2.2.0",
    "keymirror": "^0.1.1",
    "moment": "2.22.2",
    "newrelic": "4.5.1",
    "node-sass": "^4.8.3",
    "nprogress": "0.2.0",
    "object-assign": "4.1.1",
    "polished": "3.0.3",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "prerender-node": "3.2.1",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react": "^16.8.0",
    "react-datepicker": "2.0.0",
    "react-dev-utils": "^5.0.0",
    "react-dom": "^16.2.0",
    "react-facebook-login": "^4.0.1",
    "react-google-login": "^3.2.1",
    "react-loadable": "^5.3.1",
    "react-markdown": "3.3.2",
    "react-multistep": "^3.0.2",
    "react-places-autocomplete": "7.2.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-test-renderer": "16.3.2",
    "react-twitter-widgets": "1.7.1",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.7",
    "script-ext-html-webpack-plugin": "2.0.1",
    "semantic-ui-css": "^2.3.1",
    "semantic-ui-react": "^0.79.1",
    "serialize-javascript": "1.5.0",
    "serve": "10.1.1",
    "style-ext-html-webpack-plugin": "3.4.7",
    "style-loader": "0.19.0",
    "styled-components": "^3.2.5",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "start": " node scripts/start.js",
    "build": "NODE_PATH=src node scripts/build.js",
    "build:serve": "NODE_PATH=src NODE_ENV=development node server/bootstrap.js ",
    "test": "NODE_PATH=src node scripts/test.js --env=jsdom",
    "postinstall": "npm run build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/js/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.js",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testMatch": [
      "<rootDir>/src/js/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/js/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.(css|scss)$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      "react-loadable/babel"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "enzyme-to-json": "3.3.4",
    "flow-bin": "^0.69.0",
    "gh-pages": "1.1.0",
    "redux-devtools": "^3.4.1"
  },
  "homepage": "/"
}
